<div class="wrapper">
    <div class="header">
        <span class="logo-action">
            <i class="logo fas fa-arrow-circle-left" onclick="window.location='/'"></i>
            <h1><i class="logo fas fa-car"></i>ezCars</h1>
        </span>
    </div>
    <div class="content">
        <h1>User Information</h1>
        <div class="main-grid">
            <div class="lgrid">
                <p class="dtext">First Name: {{body.firstName}}</p>
                <p class="dtext">Last Name: {{body.lastName}}</p>
                <p class="dtext">Address: {{body.address}}</p>
                <p class="dtext">Email: {{body.email}}</p>
            </div>
            <div class="rgrid">
                <p class="dtext">Phone Number: {{body.phoneNum}}</p>
                <p class="dtext">License Number: {{body.licenseNum}}</p>
            </div>
        </div>
</div>