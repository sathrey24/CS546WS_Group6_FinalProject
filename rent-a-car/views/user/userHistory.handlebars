<div class="wrapper">
    <div class="header">
        <span class="logo-action">
            <i class="logo fas fa-arrow-circle-left" onclick="window.location='/userDashboard'"></i>
            <h1><i class="logo fas fa-car"></i>ezCars</h1>
        </span>
    </div>
    <div class="content">
        <h1>Past History ({{body.length}})</h1>
        {{#if body.length}}
        <form action="/review/{{this._id}}" method="post" id="review-form"></form>
        <ul class="carlist-ul">
            {{#each body}}
            <li class="cars">
                Date : {{fromDate}} to {{toDate}}<br />
                Time span : {{timePeriod}}</br>
                <span>
                    <label>Review :
                        <input name="review" type="text" id="reviewId" style="margin: 5px 0px;" />
                    </label>
                    <br />
                    <label>Rating :
                        <input name="rating" type="number" id="rateId" style="margin: 5px 0px;" />
                    </label>
                    <br />
                    <input type="submit" value="Submit" class="btn btn-dark savebtn" style="margin: 5px 0px;" />
                </span>
                <span class="carlist-action-btns">
                    <i class="tooltip action-logo fas fa-eye" onclick="window.location='/request/{{this._id}}'"
                        style="color: blue;">
                        <span class="tooltiptext">View Details</span>
                    </i>
                </span>
            </li>
            {{/each}}
        </ul>
        </form>
        {{/if}}
    </div>
</div>