<div class="wrapper">
    <div class="header">
            <div class="logo-action">
            <i class="logo fas fa-arrow-circle-left" onclick="window.location='/admin/adminDashboard'"></i>
            <h1><i class="logo fas fa-car"></i>ezCars</h1>
        </div>
        <div class="btn-grp">

            <button onclick="window.location='/admin/addCar'" class="btn btn-light-outline">Add Car<i
                    class="far fa-plus-square"></i></button>
        </div>
       
    </div>
    <div class="content">
        <h1>Cars List ({{body.length}})</h1>
        <div class="scroll-container">
            {{#if body.length}}
            <ul class="carlist-ul">
                {{#each body}}
                <li class="cars">
                    <a href="/cars/{{this._id}}">{{this.model}}</a>
                    <span class="carlist-action-btns">
                        <i class="tooltip delete fas fa-trash"
                            onclick="window.location='/admin/deleteCar/{{this._id}}'">
                            <span class="tooltiptext">Delete</span>
                        </i>
                        <i class="tooltip edit fas fa-edit" onclick="window.location='/admin/editCar/{{this._id}}'">
                            <span class="tooltiptext">Edit</span>
                        </i>
                    </span>
                </li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
    </div>
    <footer>
        <p id="time"  class="formating">1:43</p>
    </footer>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
    <script src="../../public/js/time.js"></script>
</div>