<div class="wrapper">
    <div class="header">
           <div class="logo-action">
            <i class="logo fas fa-arrow-circle-left" onclick="window.location='/admin/adminDashboard'"></i>
            <h1><i class="logo fas fa-car"></i>ezCars</h1>
        </div>
    </div>
    <div class="content">
        <h1>Rented Cars List ({{body.length}})</h1>
         <div class="scroll-container">
        {{#if body.length}}
        <ul class="carlist-ul">
            {{#each body}}
            <li class="cars">
                Rented by : {{this.username}}<br />
                Date : {{fromDate}} to {{toDate}}<br />
                Time span : {{timePeriod}}<br />
                <span class="carlist-action-btns">
                    <i class="tooltip action-logo fas fa-eye" onclick="window.location='/admin/request/{{this._id}}'"
                        style="color: blue;">
                        <span class="tooltiptext">View Details</span>
                    </i>
                </span>
            </li>
            {{/each}}
        </ul>
        {{/if}}
        </div>
        
    </div>
</div>