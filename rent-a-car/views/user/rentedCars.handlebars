<div class="wrapper">
    <div class="header">
        <span class="logo-action">
            <i class="logo fas fa-arrow-circle-left" onclick="window.location='/admin/adminDashboard'"></i>
            <h1><i class="logo fas fa-car"></i>ezCars</h1>
        </span>
    </div>
    <div class="content">
        <h1>Rented Cars List ({{body.length}})</h1>
        {{#if body.length}}
        <ul class="carlist-ul">
            {{#each body}}
            <li class="cars carlist-action-btns">
                <a href="/cars/{{this._id}}">{{this.model}}</a>
            </li>
            {{/each}}
        </ul>
        {{/if}}
    </div>
</div>